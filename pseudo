#!/usr/bin/env python
import sys

import pseudocode



HELP = f"""
Pseudo {pseudocode.__version__}
""".strip()



if __name__ == "__main__":
    srcfile = "main.pseudo"
    print(HELP)
    if len(sys.argv) > 1:
        srcfile = sys.argv[1]
    # print("""Usage: pseudo [FILE]""")
    # sys.exit(65)

    pseudocode.run(srcfile)
    